        "\n",
        "\n",
        "data['accessed_date'] = pd.to_datetime(data['accessed_date'])\n",
        "data['date'] = data['accessed_date'].dt.date\n",
        "sales_by_date = data.groupby('date')['sales'].sum()\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.plot(sales_by_date, marker='o', linestyle='-')\n",
        "plt.xlabel('accessed_date')\n",
        "plt.ylabel('returned_amount')\n",
        "plt.title('Time Trend of Sales')\n",
        "plt.xticks(rotation=45)\n",
        "plt.tight_layout()\n",
        "plt.show()"
